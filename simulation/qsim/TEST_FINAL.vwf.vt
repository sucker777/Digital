`timescale 1 ps/ 1 ps
module Poker_vlg_sample_tst(
	CLOCK,
	P1_Race,
	P1_Roll,
	P2_Race,
	P2_Roll,
	Switch,
	sampler_tx
);
input  CLOCK;
input  P1_Race;
input  P1_Roll;
input  P2_Race;
input  P2_Roll;
input  Switch;
output sampler_tx;

reg sample;
time current_time;
always @(CLOCK or P1_Race or P1_Roll or P2_Race or P2_Roll or Switch)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

